package doublylinkedlistpack;
class Node{
	int d;
	Node pre,next;
	Node(){
		pre=null;
		next=null;
	}
}
class Linkedlist
{
	Node head;
	Linkedlist(){
		head=null;
	}
	void append(int i)
	{
		Node L=new Node();
		L.d=i;
		if (head==null)
		{
			head=L;
		}
		else
		{
			Node l=head;
			while(l.next!=null)
			{
				l=l.next;
			}
			l.pre=l;
			
			l.next=L;
			
		}
	}
}
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		

	}

}
